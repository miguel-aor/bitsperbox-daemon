# BitsperBox Configuration
# Copy this file to .env and fill in the values

# Supabase Connection
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-role-key

# Device Configuration (set via 'npm run setup')
DEVICE_ID=
DEVICE_TOKEN=
RESTAURANT_ID=

# Printer Configuration
PRINTER_VENDOR_ID=
PRINTER_PRODUCT_ID=

# Optional: Network printer (if not using USB)
PRINTER_IP=
PRINTER_PORT=9100

# Logging
LOG_LEVEL=info
